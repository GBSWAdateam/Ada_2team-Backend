<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>kakaotalk login</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>
    <script src='main.js'></script>
</head>
<body>

    <a href="javascript:kakaotalkLogin()"><img src="/kakao.png" alt="카카오 로그인"></a>
    <script src="https://developers.kakao.com/sdk/js/kakao.js"></script>

    <script>
        //55e0cb9ba2778691fdc6b2f0581f242e
        window.Kakao.init("55e0cb9ba2778691fdc6b2f0581f242e");

        function kakaotalkLogin() {
            window.Kakao.Auth.login ({
                scope :'profile_nickname',
                success : function(authObj) {
                    consol.log(authObj);
                    window.Kakao.API.reqest({
                        url : 'v2/user/me', 
                        success : res => {
                            const kakao_account = res.kakao_account;
                            console.log(kakao_account);
                        }
                    })
                }
            })
        }
    </script>
</body>
</html>